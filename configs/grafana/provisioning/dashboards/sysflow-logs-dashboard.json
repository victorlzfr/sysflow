{
  "title": "SysFlow Logs Dashboard",
  "tags": ["sysflow", "logs", "monitoring"],
  "style": "dark",
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "SysFlow Application Logs",
      "type": "logs",
      "datasource": "Loki",
      "targets": [
        {
          "expr": "{job=\"sysflow\"}",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "wrapLogMessage": true,
        "showLabels": false,
        "enableLogDetails": true
      },
      "gridPos": {
        "h": 12,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Log Rate - All Sources",
      "type": "timeseries",
      "datasource": "Loki",
      "targets": [
        {
          "expr": "rate({}[5m])",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 12
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "10s"
}
